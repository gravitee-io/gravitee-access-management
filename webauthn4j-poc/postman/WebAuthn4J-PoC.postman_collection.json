{
	"info": {
		"_postman_id": "webauthn4j-poc-collection",
		"name": "WebAuthn4J PoC - Fallback Validation Demo",
		"description": "Postman collection for testing WebAuthn4J PoC with Android Key attestation and fallback certificate validation",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Health Check",
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{baseUrl}}/health",
					"host": [
						"{{baseUrl}}"
					],
					"path": [
						"health"
					]
				}
			},
			"response": []
		},
		{
			"name": "Get Configuration",
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{baseUrl}}/webauthn/config",
					"host": [
						"{{baseUrl}}"
					],
					"path": [
						"webauthn",
						"config"
					]
				}
			},
			"response": []
		},
		{
			"name": "Registration Flow",
			"item": [
				{
					"name": "Begin Registration",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"username\": \"{{username}}\"\n}"
						},
						"url": {
							"raw": "{{baseUrl}}/webauthn/register/begin",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"webauthn",
								"register",
								"begin"
							]
						}
					},
					"response": []
				},
				{
					"name": "Complete Registration (Android Key)",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"username\": \"{{username}}\",\n  \"credential\": {\n    \"id\": \"{{credentialId}}\",\n    \"rawId\": \"{{credentialId}}\",\n    \"response\": {\n      \"attestationObject\": \"{{attestationObject}}\",\n      \"clientDataJSON\": \"{{clientDataJSON}}\"\n    },\n    \"type\": \"public-key\"\n  }\n}"
						},
						"url": {
							"raw": "{{baseUrl}}/webauthn/register/complete",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"webauthn",
								"register",
								"complete"
							]
						}
					},
					"response": []
				}
			]
		},
		{
			"name": "Authentication Flow",
			"item": [
				{
					"name": "Begin Authentication",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"username\": \"{{username}}\"\n}"
						},
						"url": {
							"raw": "{{baseUrl}}/webauthn/authenticate/begin",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"webauthn",
								"authenticate",
								"begin"
							]
						}
					},
					"response": []
				},
				{
					"name": "Complete Authentication",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"username\": \"{{username}}\",\n  \"credential\": {\n    \"id\": \"{{credentialId}}\",\n    \"rawId\": \"{{credentialId}}\",\n    \"response\": {\n      \"authenticatorData\": \"{{authenticatorData}}\",\n      \"clientDataJSON\": \"{{clientDataJSON}}\",\n      \"signature\": \"{{signature}}\",\n      \"userHandle\": \"{{userHandle}}\"\n    },\n    \"type\": \"public-key\"\n  }\n}"
						},
						"url": {
							"raw": "{{baseUrl}}/webauthn/authenticate/complete",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"webauthn",
								"authenticate",
								"complete"
							]
						}
					},
					"response": []
				}
			]
		},
