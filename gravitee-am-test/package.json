{
  "name": "gravitee-am-test",
  "version": "3.18.0-SNAPSHOT",
  "private": true,
  "description": "Gravitee.io AM - Tests",
  "scripts": {
    "update:sdk:mapi": "bash scripts/update-management-api-sdk.sh $*",
    "test": "jest --runInBand --no-cache --config=api/config/dev.config.js",
    "ci": "jest --runInBand --no-cache --config=api/config/ci.config.js",
    "ui-test": "cypress open",
    "lint": "echo 'Lint to be configured'",
    "lint:fix": "npm run prettier:fix && npm run lint:license:fix",
    "lint:license": "license-check-and-add check -f license-check-config.json",
    "lint:license:fix": "license-check-and-add add -f license-check-config.json -r",
    "prettier": "prettier --check \"**/*.{js,ts,html,css,scss,json}\"",
    "prettier:fix": "prettier --write \"**/*.{js,ts,html,css,scss,json}\"",
    "prettier:list": "prettier --list-different \"**/*.{js,ts,html,css,scss,json}\""
  },
  "license": "MIT License",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@jest/globals": "^28.0.3",
    "@types/faker": "5.5.9",
    "@types/jest": "^27.5.0",
    "@types/node": "^14.18.5",
    "ansi-regex": "6.0.1",
    "babel-jest": "^28.0.3",
    "btoa": "^1.2.1",
    "cheerio": "^1.0.0-rc.12",
    "cypress": "^8.7.0",
    "cypress-terminal-report": "3.4.1",
    "har-validator": "5.1.5",
    "jest": "^28.0.3",
    "license-check-and-add": "^4.0.3",
    "prettier": "2.5.1",
    "supertest": "^6.2.3",
    "test-jest": "^1.0.1",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "faker": "5.5.3"
  }
}
